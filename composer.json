{
    "name": "na/mock-check",
    "description": "PHPStan plugin for mock checks",
    "type": "phpstan-extension",
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0",
        "psr/log": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Na\\MockCheck\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Na\\MockCheck\\Tests\\": "tests/",
            "App\\": "tests/Stubs/App/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse"
    }
}
